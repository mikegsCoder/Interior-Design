{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=InteriorDesign;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "RequireConfirmedAccount": "false",
    "RequireConfirmedEmail": "false",
    "DefaultLockoutTimeSpan": "30",
    "MaxFailedAccessAttempts": "20",
    "AllowedForNewUsers": "true",
    "RequireDigit": "true",
    "RequireLowercase": "true",
    "RequireNonAlphanumeric": "true",
    "RequireUppercase": "true",
    "RequiredLength": "6",
    "RequiredUniqueChars": "1"
  },
  "ApplicationCookie": {
    "ExpireTimeSpan": "30",
    "SlidingExpiration": "true"
  },
  "DataProtectionTokenProvider": {
    "TokenLifespan": "1"
  },
  "CookiePolicyOptions": {
    "CheckConsentNeeded": "true"
  },
  "Contacts": {
    "Email": "InteriorDesign@mail.com",
    "Address": "\"Dubrovnik\" Street, Varna 9010, Bulgaria",
    "Phone": "+359 123 456 789"
  }
}
